From ee1be58024c64937c273c12e891bc7349e5f91b9 Mon Sep 17 00:00:00 2001
From: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Date: Mon, 28 Nov 2022 15:21:55 +0200
Subject: [PATCH 28/54] s32cc: Sync ATF and Linux GPIO nodes

Issue: ALB-9425
Upstream-Status: Pending 

Signed-off-by: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 fdts/s32g.dtsi | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/fdts/s32g.dtsi b/fdts/s32g.dtsi
index 1677231cb..da4a45ad2 100644
--- a/fdts/s32g.dtsi
+++ b/fdts/s32g.dtsi
@@ -114,7 +114,7 @@
 			};
 
 			gpio: siul2-gpio@4009d700 {
-				compatible = "nxp,s32cc-siul2-gpio";
+				compatible = "nxp,s32g-siul2-gpio", "nxp,s32cc-siul2-gpio";
 
 				reg = <OPADS_SIUL2_0        0 0x0 0x10>,
 				      <OPADS_SIUL2_1        0 0x0 0x18>,
@@ -131,6 +131,8 @@
 				gpio-ranges = <&pinctrl 0   0   102>,
 					/* GPIO 112-190 */
 					      <&pinctrl 112 112 79>;
+				gpio-reserved-ranges = <102 10>,
+						       <123 21>;
 				status = "okay";
 			};
 
-- 
2.25.1

