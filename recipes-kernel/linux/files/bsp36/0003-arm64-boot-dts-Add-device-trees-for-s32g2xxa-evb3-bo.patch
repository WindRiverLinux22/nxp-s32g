Subject: [PATCH 03/25] arm64/boot/dts: Add device trees for s32g2xxa-evb3
 board

Issue: ALB-9666
Upstream-Status: Pending 

Signed-off-by: Ghennadi Procopciuc <ghennadi.procopciuc@nxp.com>
Signed-off-by: Zhantao Tang <zhantao.tang@windriver.com>
---
 arch/arm64/boot/dts/freescale/Makefile        |  2 ++
 .../dts/freescale/s32g2xxa-evb3-pfems.dts     | 15 +++++++++++
 .../boot/dts/freescale/s32g2xxa-evb3.dts      |  6 +++++
 .../boot/dts/freescale/s32g2xxa-evb3.dtsi     | 25 +++++++++++++++++++
 4 files changed, 48 insertions(+)
 create mode 100644 arch/arm64/boot/dts/freescale/s32g2xxa-evb3-pfems.dts
 create mode 100644 arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dts
 create mode 100644 arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dtsi

diff --git a/arch/arm64/boot/dts/freescale/Makefile b/arch/arm64/boot/dts/freescale/Makefile
index f2658cb1cd00..10bea8622330 100644
--- a/arch/arm64/boot/dts/freescale/Makefile
+++ b/arch/arm64/boot/dts/freescale/Makefile
@@ -75,6 +75,8 @@ dtb-$(CONFIG_ARCH_S32) += s32g274a-bluebox3.dtb
 dtb-$(CONFIG_ARCH_S32) += s32g274a-emu.dtb
 dtb-$(CONFIG_ARCH_S32) += s32g2xxa-evb.dtb
 dtb-$(CONFIG_ARCH_S32) += s32g2xxa-evb-pfems.dtb
+dtb-$(CONFIG_ARCH_S32) += s32g2xxa-evb3.dtb
+dtb-$(CONFIG_ARCH_S32) += s32g2xxa-evb3-pfems.dtb
 dtb-$(CONFIG_ARCH_S32) += s32g3xxa-evb.dtb
 dtb-$(CONFIG_ARCH_S32) += s32g3xxa-evb-pfems.dtb
 dtb-$(CONFIG_ARCH_S32) += s32g3xxa-evb3.dtb
diff --git a/arch/arm64/boot/dts/freescale/s32g2xxa-evb3-pfems.dts b/arch/arm64/boot/dts/freescale/s32g2xxa-evb3-pfems.dts
new file mode 100644
index 000000000000..eb01cafe0079
--- /dev/null
+++ b/arch/arm64/boot/dts/freescale/s32g2xxa-evb3-pfems.dts
@@ -0,0 +1,15 @@
+// SPDX-License-Identifier: GPL-2.0-or-later
+/*
+ * Copyright 2023 NXP
+ */
+
+#include "s32g2xxa-evb3.dtsi"
+#include "s32g-pfe-slave.dtsi"
+
+&pfe_aux0 {
+	status = "okay";
+};
+
+&pfesl_aux0 {
+	status = "okay";
+};
diff --git a/arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dts b/arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dts
new file mode 100644
index 000000000000..827722cb0245
--- /dev/null
+++ b/arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dts
@@ -0,0 +1,6 @@
+// SPDX-License-Identifier: GPL-2.0-or-later
+/*
+ * Copyright 2023 NXP
+ */
+
+#include "s32g2xxa-evb3.dtsi"
diff --git a/arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dtsi b/arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dtsi
new file mode 100644
index 000000000000..669e5e0ef9c9
--- /dev/null
+++ b/arch/arm64/boot/dts/freescale/s32g2xxa-evb3.dtsi
@@ -0,0 +1,25 @@
+// SPDX-License-Identifier: GPL-2.0-or-later
+/*
+ * Copyright 2023 NXP
+ */
+
+/dts-v1/;
+#include "s32g2.dtsi"
+#include "s32gxxxa-evb.dtsi"
+
+/ {
+	model = "NXP S32G2XXX-EVB3";
+};
+
+&pfe_mdio2 {
+	/* AQR113 on S32G-PROCEVB3-S */
+	pfe_mdio_a_phy8: ethernet-phy@8 {
+		compatible = "ethernet-phy-ieee802.3-c45";
+		reg = <8>;
+	};
+};
+
+&pfe_netif0 {
+	phy-mode = "sgmii";
+	phy-handle = <&pfe_mdio_a_phy8>;
+};
