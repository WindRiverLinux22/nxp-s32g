+									config->emac_mode[2], EMAC_SPEED_1000_MBPS, EMAC_DUPLEX_FULL);
+			if (NULL == platform->emac[2])
+			{
+				NXP_LOG_ERROR("Couldn't create EMAC3 instance\n");
+				ret = ENODEV;
+			}
+		}
+	}
+
+	return ret;
+}
+
+/**
+ * @brief		Release EMAC-related resources
